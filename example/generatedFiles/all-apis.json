[
    {
        "api": "${BASE_URL}/usuarios",
        "method": "POST",
        "headers": {
            "Content-Type": "application/json"
        },
        "description": "Register a new user",
        "body": {
            "nome": "Rishad Omar",
            "email": "rishadomar@gmail.com",
            "password": "testing123",
            "administrador": "true"
        },
        "expectedResponse": {
            "message": "Cadastro realizado com sucesso",
            "_id": "*"
        },
        "category": "Users",
        "filename": "example/Users/1-registerUser.json",
        "name": "1-registerUser",
        "id": 1
    },
    {
        "api": "${BASE_URL}/usuarios",
        "method": "POST",
        "headers": {
            "Content-Type": "application/json"
        },
        "description": "Register a new user with duplicate email",
        "body": {
            "nome": "Another user",
            "email": "rishadomar@gmail.com",
            "password": "testing123",
            "administrador": "true"
        },
        "expectedResponseId": 400,
        "expectedResponse": {
            "message": "Este email já está sendo usado"
        },
        "category": "Users",
        "filename": "example/Users/2-registerDuplicateUser.json",
        "name": "2-registerDuplicateUser",
        "id": 2
    },
    {
        "api": "${BASE_URL}/usuarios?nome=Rishad Om",
        "method": "GET",
        "headers": {
            "Accept": "application/json"
        },
        "description": "List registered users",
        "expectedResponse": {
            "quantidade": 1,
            "usuarios": [
                {
                    "nome": "Rishad Omar",
                    "email": "rishadomar@gmail.com",
                    "password": "testing123",
                    "administrador": "true",
                    "_id": "*"
                }
            ]
        },
        "category": "Users",
        "filename": "example/Users/3-getListOfUsers.json",
        "name": "3-getListOfUsers",
        "id": 3
    },
    {
        "api": "${BASE_URL}/usuarios/${PREVIOUS_RESULT.1-registerUser._id}",
        "method": "PUT",
        "headers": {
            "Content-Type": "application/json"
        },
        "description": "Update user's password",
        "body": {
            "nome": "Rishad Omar",
            "email": "rishadomar@gmail.com",
            "password": "testing123456",
            "administrador": "false"
        },
        "expectedResponse": {
            "message": "Registro alterado com sucesso"
        },
        "category": "Users",
        "filename": "example/Users/4-updateUser.json",
        "name": "4-updateUser",
        "id": 4
    },
    {
        "api": "${BASE_URL}/usuarios/${PREVIOUS_RESULT.1-registerUser._id}",
        "method": "DELETE",
        "headers": {
            "Content-Type": "application/json"
        },
        "description": "Delete user",
        "expectedResponse": {
            "message": "Registro excluído com sucesso"
        },
        "category": "Users",
        "filename": "example/Users/5-deleteUser.json",
        "name": "5-deleteUser",
        "id": 5
    }
]