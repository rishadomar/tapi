    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/utils/generateCypressTests.js",
            "cwd": "${workspaceFolder}",
            "name": "generate cypress tests",
            "args": ["example"]
        },
        {
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/src/stringUtils.js",
            "cwd": "${workspaceFolder}",
            "name": "string utils",
            "args": ["src/data"]
        },
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}"
        }
    ]
}
